      }
    ],
    "grammars": [
      {
        "language": "uims",
        "scopeName": "source.componentmodel",
        "path": "./syntaxes/uims.tmLanguage.json"
      }
    ]
  },
  "dependencies": {
    "@vscode/vsce": "^3.1.1"
  }
}
